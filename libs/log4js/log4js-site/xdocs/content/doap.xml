    <title>Log4js</title>
    <id>tag:log4js:0.7</id>
    <updated>2005-08-09T23:06:00Z</updated>

    <content type="application/rdf+xml">
      <doap:Project>
        <doap:name>Log4js</doap:name>
        <doap:homepage rdf:resource="http://log4js.berlios.de"/>
        <doap:created>2005-06-23</doap:created>

        <doap:description xml:lang="en">
          Log4js is a logging API for JavaScript. The main goal is to have 
          a robust and solid logging API which is very simmilar to the 
          Java logging API <a href="ext:log4j.apache.org">Log4j</a>.
        </doap:description>

        <doap:programming-language xml:lang="en">JavaScript</doap:programming-language>
        <doap:programming-language xml:lang="en">XML</doap:programming-language>

        <doap:license rdf:resource="http://www.apache.org/licenses/LICENSE-2.0.txt"/>

        <doap:download-page
          rdf:resource="http://developer.berlios.de/project/showfiles.php?group_id=5368"/>

        <doap:mailing-list
          rdf:resource="http://log4js.berlios.de/project/maillists.html"/>

        <doap:maintainer>
          <foaf:Person>
            <foaf:name>The Log4js Project</foaf:name>
            <foaf:homepage rdf:resource="http://log4js.berlios.de/comitter.html"/>
<!--            <foaf:mbox rdf:resource="mailto:dev@forrest.apache.org"/>-->
          </foaf:Person>
        </doap:maintainer>

        <doap:release>
          <doap:Version>
            <doap:revision>0.2</doap:revision>
            <doap:created>unreleased</doap:created>
          </doap:Version>
        </doap:release>
